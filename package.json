{
  "name": "ads-roi",
  "version": "1.0.0",
  "description": "Interactive widget for calculating advertising campaign ROI with real-time sliders",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudflare-mcp-apps/ads-roi.git"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cloudflare-workers",
    "mcp-apps",
    "sep-1865"
  ],
  "overrides": {
    "iron-session": {
      "cookie": "^0.7.0"
    }
  },
  "scripts": {
    "dev": "wrangler dev",
    "dev:widget": "cross-env INPUT=widgets/dashboard.html vite build --watch",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:widget\"",
    "build": "cross-env INPUT=widgets/dashboard.html vite build",
    "build:widgets": "npm run build",
    "watch": "cross-env INPUT=widgets/dashboard.html vite build --watch",
    "watch:widgets": "npm run watch",
    "deploy": "npm run build:widgets && wrangler deploy",
    "type-check": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "serve:http": "wrangler dev --port 3000",
    "serve:stdio": "wrangler dev --experimental-enable-local-persistence"
  },
  "dependencies": {
    "@cloudflare/workers-oauth-provider": "^0.1.0",
    "@modelcontextprotocol/ext-apps": "^0.2.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@workos-inc/node": "^7.77.0",
    "agents": "^0.2.30",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "hono": "^4.10.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-singlefile": "^2.3.0",
    "wrangler": "^4.45.3"
  }
}
